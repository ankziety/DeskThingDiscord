{
  "name": "discordthing",
  "private": true,
  "version": "0.1.3-0",
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "Ankziety",
  "readme": "./README.md",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build-server",
    "build-server": "esbuild server/index.ts --bundle --platform=node --allow-overwrite --outdir=dist/",
    "postbuild": "distro_zip.bat && npm run docs",
    "docs": "npx typedoc --entryPointStrategy Expand server src",
    "test": "npx vitest",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ankziety/discord-rpc": "^4.0.5",
    "deskthing-client": "^0.10.0",
    "deskthing-server": "^0.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-legacy": "^5.4.2",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "esbuild": "^0.19.2",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.14.0",
    "tailwindcss": "^3.4.10",
    "typedoc": "^0.27.6",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.22.0",
    "vite": "^5.4.1",
    "vitest": "^3.0.4"
  }
